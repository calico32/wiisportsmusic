{
  "name": "wiisportsmusic-2",
  "version": "2.0.0-alpha+1",
  "description": "a discord music bot",
  "scripts": {
    "prod": "pnpm run build:prod && pnpm run server:prod",
    "build:prod": "concurrently -n webpack:prod -c \"white.bgBlue.bold\" \"webpack -p\"",
    "server:prod": "concurrently -n node:prod -c \"white.bgGreen.bold\" \"node ./dist/bot.js\"",
    "dev": "pnpm run build:dev-color && concurrently -n \"webpack,nodemon\" -c \"white.bgBlue.bold,white.bgGreen.bold\" -i \"pnpm run build:watch\" \"pnpm run server:watch\"",
    "server:watch": "nodemon \"./dist/bot.js\"",
    "build:watch": "webpack -d --watch",
    "build:dev-color": "concurrently -n webpack -c \"white.bgBlue.bold\" \"webpack -d\""
  },
  "repository": {
    "directory": "./"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "dist"
    ]
  },
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@discordjs/opus": "^0.1.0",
    "bufferutil": "^4.0.1",
    "concurrently": "^5.1.0",
    "discord.js": "^12.0.2",
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0",
    "simple-youtube-api": "^5.2.1",
    "utf-8-validate": "^5.0.2",
    "ytdl-core": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^12.12.31",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
